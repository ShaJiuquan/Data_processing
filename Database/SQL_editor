SELECT * FROM "STMdataLists" where Date="18.11.2022";
SELECT PosX_nm, PosY_nm from STMdataLists  WHERE UpdateFilePath='../../../STMdata/20221116/SiC028.sxm';

SELECT SCAN_RANGE from STMimageInfo WHERE List_ID=5836;

SELECT SCAN_RANGE from STMimageInfo WHERE List_ID=5836;

SELECT *
FROM STMdataLists
JOIN STMimageLabel ON STMdataLists.List_ID = STMimageLabel.List_ID
WHERE STMdataLists.List_ID  = 150;
SELECT * FROM STMdataLists
JOIN STMimageLabel ON STMdataLists.List_ID = STMimageLabel.List_ID
WHERE STMdataLists.List_ID  = 1;
SELECT * FROM STMdataLists WHERE STMdataLists.List_ID  = 50;

SELECT * FROM STMdataLists , STMimageLabel WHERE STMdataLists.List_ID  = 1 AND STMimageLabel.List_ID=1;
SELECT * FROM STMdataLists , STMimageLabel WHERE STMdataLists.List_ID = STMimageLabel.List_ID AND STMdataLists.List_ID <10

